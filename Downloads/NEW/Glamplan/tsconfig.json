{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react-native",
      "outDir": "./dist",
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
  }
  